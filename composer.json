{
  "name": "rocksolid/flysystem-azure-blob-storage",
  "description": "Azure Blob Storage adapter for Flysystem v3",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "azure",
    "blob-storage",
    "flysystem",
    "storage"
  ],
  "authors": [
    {
      "name": "Rocksolid Development",
      "email": "hello@rocksolid.dev"
    }
  ],
  "require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.0",
    "league/flysystem": "^3.0",
    "league/mime-type-detection": "^1.16.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "league/flysystem-adapter-test-utilities": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Rocksolid\\Flysystem\\AzureBlobStorage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rocksolid\\Flysystem\\AzureBlobStorage\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
